<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".adminActivity">

    <TableLayout
        android:id="@+id/dbOutput"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/Transition"
        app:layout_constraintEnd_toEndOf="parent">

    </TableLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/Transition"
        android:layout_width="0dp"
        android:layout_height="50dp"

        android:layout_marginRight="5dp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_style"
        android:textSize="21dp"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:text="Пользователи"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/btnToMain"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/dbOutput"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnToMain"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="5dp"
        android:textSize="21dp"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:background="@drawable/button_style"
        android:text="На главную"
        app:layout_constraintStart_toEndOf="@id/Transition"
        app:layout_constraintTop_toBottomOf="@id/dbOutput"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>